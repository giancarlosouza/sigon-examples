communication:
    sensor("positionSensor", "sensor.PositionSensor").
    actuator("next", "actuator.NextSlot").
    actuator("toDodge", "actuator.ToDodge").

desires:
	check(slots).
	not obstacle.
	
intentions:
	end.
	
planner:
	plan(check(slots), [action(next(slot))], [not obstacle, not end], _).
	plan(check(slots), [action(toDodge(garbage))], [obstacle, not end], not obstacle).


